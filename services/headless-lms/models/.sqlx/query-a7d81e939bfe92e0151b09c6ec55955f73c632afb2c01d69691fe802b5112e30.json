{
  "db_name": "PostgreSQL",
  "query": "\nSELECT g.id,\n  g.created_at,\n  g.updated_at,\n  g.deleted_at,\n  g.organization_id,\n  g.name\nFROM groups g\n  JOIN group_memberships gm ON gm.group_id = g.id\nWHERE g.organization_id = $1\n  AND g.deleted_at IS NULL\n  AND gm.user_id = $2\n  AND gm.deleted_at IS NULL\nORDER BY LOWER(g.name),\n  g.id\n",
